{
  "rules": {
    "users": {
      "$uid": {
        ".write": "$uid === auth.uid"
      }
    },
    "flashcard-sets": {
      "$flashcard": {
        ".write": "data.child('AuthorID').val() === auth.uid"
      }
    },
    "$setid": {
      ".write": "root.child('flashcard-sets/' + $setid).child('AuthorID').val() === auth.uid"
    },
    ".read": true,
    ".write": false
  }
}
